{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "nestjs-starter": {
      "root": "apps/nestjs-starter",
      "sourceRoot": "apps/nestjs-starter/src",
      "projectType": "application",
      "prefix": "",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/nestjs-starter",
            "main": "apps/nestjs-starter/src/main.ts",
            "tsConfig": "apps/nestjs-starter/tsconfig.app.json",
            "assets": [
              "apps/nestjs-starter/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/nestjs-starter/src/environments/environment.ts",
                  "with": "apps/nestjs-starter/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "nestjs-starter:build",
            "inspect": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/nestjs-starter/tsconfig.app.json",
              "apps/nestjs-starter/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/nestjs-starter/jest.config.js",
            "tsConfig": "apps/nestjs-starter/tsconfig.spec.json"
          }
        }
      }
    },
    "angular-starter": {
      "root": "apps/angular-starter/",
      "sourceRoot": "apps/angular-starter/src",
      "projectType": "application",
      "prefix": "",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angular-starter",
            "index": "apps/angular-starter/src/index.html",
            "main": "apps/angular-starter/src/main.ts",
            "polyfills": "apps/angular-starter/src/polyfills.ts",
            "tsConfig": "apps/angular-starter/tsconfig.app.json",
            "assets": [
              "apps/angular-starter/src/favicon.ico",
              "apps/angular-starter/src/assets"
            ],
            "styles": [
              "apps/angular-starter/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular-starter/src/environments/environment.ts",
                  "with": "apps/angular-starter/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-starter:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-starter:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-starter:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/angular-starter/src/test.ts",
            "polyfills": "apps/angular-starter/src/polyfills.ts",
            "tsConfig": "apps/angular-starter/tsconfig.spec.json",
            "karmaConfig": "apps/angular-starter/karma.conf.js",
            "styles": [
              "apps/angular-starter/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "apps/angular-starter/src/favicon.ico",
              "apps/angular-starter/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular-starter/tsconfig.app.json",
              "apps/angular-starter/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "angular-starter-e2e": {
      "root": "apps/angular-starter-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/angular-starter-e2e/protractor.conf.js",
            "devServerTarget": "angular-starter:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-starter:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/angular-starter-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "shared": {
      "root": "libs/shared",
      "sourceRoot": "libs/shared/src",
      "projectType": "library",
      "prefix": "",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/shared/src/test.ts",
            "tsConfig": "libs/shared/tsconfig.spec.json",
            "karmaConfig": "libs/shared/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/shared/tsconfig.lib.json",
              "libs/shared/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      },
      "schematics": {}
    },
    "airport": {
      "root": "apps/airport",
      "sourceRoot": "apps/airport/src",
      "projectType": "application",
      "prefix": "airport",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/airport",
            "main": "apps/airport/src/main.ts",
            "tsConfig": "apps/airport/tsconfig.app.json",
            "assets": [
              "apps/airport/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/airport/src/environments/environment.ts",
                  "with": "apps/airport/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "airport:build",
            "inspect": false
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/airport/tsconfig.app.json",
              "apps/airport/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/airport/jest.config.js",
            "tsConfig": "apps/airport/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "defaultProject": "angular-starter",
  "schematics": {
    "@nrwl/schematics:library": {
      "unitTestRunner": "karma",
      "framework": "angular"
    },
    "@nrwl/schematics:application": {
      "unitTestRunner": "karma",
      "e2eTestRunner": "protractor"
    },
    "@nrwl/schematics:node-application": {
      "framework": "express"
    }
  }
}
